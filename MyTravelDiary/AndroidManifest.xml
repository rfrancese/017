            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="it.unisa.mytraveldiary.MasterTravelMessageActivity" />
        </activity>
        <activity
            android:name="it.unisa.mytraveldiary.SearchTravelActivity"
            android:label="@string/title_activity_search_travel_message"
            android:parentActivityName="it.unisa.mytraveldiary.MasterTravelActivity" >
